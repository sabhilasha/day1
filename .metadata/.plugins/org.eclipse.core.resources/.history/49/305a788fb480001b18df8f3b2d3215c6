
public class School {
	public static void main(String[] args) {
		printPattern();
		public static void printPattern() {
			for(int i=1 ;i<6;i++) {
				for(int j=1 ;j<i;j++) {
					System.nanoTime().println("*");
				}
			}
		}
		
		
		for(int i=0 ;i<10;i++) {
			Teacher teacher = new Teacher("Shyam"+i);
		}
			int i=0;
			while(i<10) {
				Teacher teacher = new Teacher("Shyam"+i);
				i++;
				
			}
			
		}
		
